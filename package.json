{
  "name": "Electron",
  "version": "1.0.0",
  "description": "Electron Webpack starter",
  "main": "electron.js",
  "scripts": {
    "build:dev": "rimraf dist && webpack -d --progress --config config/webpack.dev.js",
    "build:prod": "rimraf dist && webpack -p --progress --config config/webpack.prod.js",
    "start": "webpack-dev-server -d --progress --config config/webpack.dev.js",
    "test": "karma start config/karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "protractor config/protractor.conf.js",
    "tslint": "tslint -p tsconfig.json -t verbose ./src/**/*.ts",
    "electron:dev": "npm run build:dev && electron ./dist/electron.js",
    "electron:prod": "npm run build:prod && electron ./dist/electron.js",
    "package:windows": "npm run build:prod && npx electron-builder build --windows",
    "package:linux": "npm run build:prod && npx electron-builder build --linux",
    "package:mac": "npm run build:prod && npx electron-builder build --mac"
  },
  "author": "Iulian Stan",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^8.2.0",
    "@angular/core": "^8.2.0",
    "@angular/platform-browser": "^8.2.0",
    "@angular/platform-browser-dynamic": "^8.2.0",
    "core-js": "^2.6.9",
    "rxjs": "^6.3.1",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular/compiler": "^8.2.0",
    "@types/jasmine": "^3.3.5",
    "@types/jasminewd2": "^2.0.6",
    "@types/webpack-env": "^1.13.6",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.1.0",
    "electron": "^9.1.0",
    "electron-builder": "^21.2.0",
    "file-loader": "^4.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jasmine": "^3.3.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "node-sass": "^4.11.0",
    "protractor": "^5.4.2",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "to-string-loader": "^1.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  }
}
